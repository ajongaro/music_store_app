<h1>All Stores Index</h1>
Home > Stores<br>
<p>
<%= link_to "New Store", "/stores/new" %>
<% @stores.each do |store| %>
  <p><b><%= store.name %></b> | Created at: <%= store.created_at %> | <%= link_to "Edit Store", "/stores/#{store.id}/edit" %>
  | <%= link_to "Delete Store", "/stores/#{store.id}", method: :delete %></p>
<% end %>
